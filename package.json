{
  "name": "tail-me",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "main": "server.js",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mysql2": "^1.6.4",
    "node-sass": "^4.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "sequelize": "^4.42.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "sass": "node-sass client/src/sass/main.scss client/src/index.css -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksonSabol/Tail-Me.git"
  },
  "bugs": {
    "url": "https://github.com/JacksonSabol/Tail-Me/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
